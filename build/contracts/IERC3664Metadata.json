  "devdoc": {
    "details": "Interface for the optional metadata functions from the ERC3664 standard.",
    "kind": "dev",
    "methods": {
      "attach(uint256,uint256,uint256,bytes,bool)": {
        "details": "Attaches `amount` value of attribute type `attrId` to `tokenId`."
      },
      "attrURI(uint256)": {
        "details": "Returns the Uniform Resource Identifier (URI) for `attrId` attribute."
      },
      "attributesOf(uint256)": {
        "details": "Returns all attribute types of owned by `tokenId`."
      },
      "balanceOf(uint256,uint256)": {
        "details": "Returns the attribute type `attrId` value owned by `tokenId`."
      },
      "balanceOfBatch(uint256,uint256[])": {
        "details": "Returns the batch of attribute type `attrIds` values owned by `tokenId`."
      },
      "batchAttach(uint256,uint256[],uint256[],bytes[])": {
        "details": "[Batched] version of {attach}."
      },
      "name(uint256)": {
        "details": "Returns the name of the attribute."
      },
      "primaryAttributeOf(uint256)": {
        "details": "Returns primary attribute type of owned by `tokenId`."
      },
      "supportsInterface(bytes4)": {
        "details": "Returns true if this contract implements the interface defined by `interfaceId`. See the corresponding https://eips.ethereum.org/EIPS/eip-165#how-interfaces-are-identified[EIP section] to learn more about how these ids are created. This function call must use less than 30 000 gas."
      },
      "symbol(uint256)": {
        "details": "Returns the symbol of the attribute."
      }
    },
    "version": 1
  },
